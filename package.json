{
  "name": "tutorbot",
  "version": "1.0.0",
  "description": "A simple hubot that can look up mtg cards",
  "main": "index.js",
  "scripts": {
    "shell": "hubot --adapter shell --disable-httpd",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=7.10.1"
  },
  "eslintConfig": {
    "rules": {
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "brace-style": 2,
      "eol-last": 2,
      "no-mixed-spaces-and-tabs": 2,
      "quotes": [
        2,
        "single"
      ],
      "semi": 2,
      "spaced-comment": [
        2,
        "always"
      ],
      "wrap-regex": 2
    }
  },
  "keywords": [
    "hubot",
    "hubot-scripts"
  ],
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0"
  },
  "dependencies": {
    "hubot": "2.19.0",
    "hubot-slack": "4.3.4",
    "lodash": "4.17.4",
  }
}
